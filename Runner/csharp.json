{
  "id" : "csharp",
  "version" : "0.8.0",
  "description": "C# support for gauge",
  "install": {
    "windows": [],
    "linux": [],
    "darwin": []
  },
  "run": {
    "windows": ["bin\\Gauge.Csharp.Runner.exe", "--start"],
    "linux": ["mono", "bin/Gauge.CSharp.Runner.exe", "--start"],
    "darwin": ["mono", "bin/Gauge.Csharp.Runner.exe", "--start"]
  },
  "init": {
    "windows": ["bin\\Gauge.Csharp.Runner.exe", "--init"],
    "linux": ["mono", "bin/Gauge.CSharp.Runner.exe", "--init"],
    "darwin": ["mono", "bin/Gauge.Csharp.Runner.exe", "--init"]
  },
  "gaugeVersionSupport": {
      "minimum": "0.5.0",
      "maximum": "0.5.0"
  }
}
